<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads Kerio Connect Client Client</string>
    
    <key>Identifier</key>
    <string>com.github.peetinc.munki.KerioConnectClient</string>
    
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>KerioConnectClient</string>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps/Kerio</string>
   		<key>CATAGORY</key>
        <string>Internet</string>
        <key>DEVELOPER</key>
        <string>Kerio (GFI)</string>
    	<key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
            <key>description</key>
            <string>Kerio Connect Client is used to connect to any Kerio Connect mail server.</string>
            <key>display_name</key>
            <string>Kerio Connect Client</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>unattended_install</key>
            <true/>
            <key>category</key>
			<string>%CATAGORY%</string>
			<key>developer</key>
			<string>%DEVELOPER%</string>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.4.0</string>
    <key>ParentRecipe</key>
    <string>com.github.peetinc.download.KerioConnectClient</string>
    <key>Process</key>
        <array>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>input_plist_path</key>
                <string>%pathname%/Kerio Connect.app/Contents/Info.plist</string>
				<key>plist_version_key</key>
				<string>CFBundleVersion</string>
        	</dict>
        	<key>Processor</key>
        	<string>Versioner</string>
        </dict>
        <dict>
            <key>Arguments</key>
            <dict>
            	<key>version_comparison_key</key>
				<string>CFBundleVersion</string>
                <key>pkg_path</key>
                <string>%RECIPE_CACHE_DIR%/downloads/%NAME%-%VERSION%.dmg</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
        </dict>
    </array>
</dict>
</plist>