<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
   <dict>
      <key>Description</key>
      <string>Downloads Kerio Connect Client Client</string>
      <key>Identifier</key>
      <string>com.github.peetinc.munki.KerioConnectClient</string>
      <key>Input</key>
      <dict>
         <key>NAME</key>
         <string>KerioConnectClient</string>
         <key>MUNKI_REPO_SUBDIR</key>
         <string>apps/Kerio</string>
         <key>DEVELOPER</key>
         <string>Kerio (GFI)</string>
         <key>CATEGORY</key>
         <string>Internet</string>
         <key>pkginfo</key>
         <dict>
            <key>catalogs</key>
            <array>
               <string>testing</string>
            </array>
            <key>description</key>
            <string>Kerio Connect Client is used to connect to any Kerio Connect mail server.</string>
            <key>display_name</key>
            <string>Kerio Connect Client</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>developer</key>
            <string>%DEVELOPER%</string>
            <key>category</key>
            <string>%CATEGORY%</string>
            <key>unattended_install</key>
            <true/>
         </dict>
      </dict>
      <key>MinimumVersion</key>
      <string>0.4.0</string>
      <key>ParentRecipe</key>
      <string>com.github.peetinc.download.KerioConnectClient</string>
      <key>Process</key>
      <array>
      		<dict>
                <key>Processor</key>
                <string>DeprecationWarning</string>
                <key>Arguments</key>
                <dict>
                    <key>warning_message</key>
                    <string>This recipe is now non-functional. Please remove it from your list of recipes. Artichoke Consulting no longer supports or recommends any Kerio products. YMMV.</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>StopProcessingIf</string>
                <key>Arguments</key>
                <dict>
                    <key>predicate</key>
                    <string>TRUEPREDICATE</string>
                </dict>
            </dict>
         <dict>
            <key>Arguments</key>
            <dict>
               <key>additional_pkginfo</key>
               <dict>
                  <key>version</key>
                  <string>%version%</string>
               </dict>
            </dict>
            <key>Processor</key>
            <string>MunkiPkginfoMerger</string>
         </dict>
         <dict>
            <key>Arguments</key>
            <dict>
               <key>version_comparison_key</key>
               <string>CFBundleVersion</string>
               <key>pkg_path</key>
               <string>%pathname%</string>
               <key>repo_subdirectory</key>
               <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
         </dict>
      </array>
   </dict>
</plist>